{
  "name": "lambda-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf out",
    "build-dev": "esbuild src/index.ts --bundle --sourcemap --platform=node --target=es2020 --outfile=out/index.js",
    "build": "esbuild src/index.ts --bundle --minify --keep-names --platform=node --target=es2020 --outfile=out/index.js",
    "postbuild": "cd out && zip -r index.zip index.js*",
    "postbuild-windows": "cd out && tar -a -cf index.zip index.js*"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.143",
    "esbuild": "^0.23.1"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.633.0",
    "aws-lambda": "^1.0.7"
  }
}
